<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">


    <LinearLayout
        android:layout_width="@dimen/location_icon_with"
        android:layout_height="@dimen/location_icon_with"
        android:background="@drawable/m_location"
        android:gravity="center"
        android:orientation="vertical">


        <com.dzrcx.jiaan.widget.CustomRoundImageView
            android:id="@+id/mlocation_user"
            android:layout_width="@dimen/location_icon_iamge_with"
            android:layout_height="@dimen/location_icon_iamge_with"
            android:layout_gravity="center"
            android:scaleType="centerInside"
            android:src="@drawable/location_user_bg" />


    </LinearLayout>


</LinearLayout>